		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.example</groupId>
	<artifactId>ethcrawler</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>eth-crawler</name>
	<description>Ethereum Intern App</description>
	<url/>
	<licenses>
		<license/>
	</licenses>
	<developers>
		<developer/>
	</developers>
	<scm>
		<connection/>
		<developerConnection/>
		<tag/>
		<url/>
